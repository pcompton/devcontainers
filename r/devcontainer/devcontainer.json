{
  "image": "philcompton/devcontainer-r:latest",
  "containerUser": "vscode",
  "customizations": {
    "vscode": {
      // Add the IDs of extensions you want installed when the container is created.
      "extensions": ["reditorsupport.r"],
      "settings": {
        "terminal.integrated.shell.*": "/usr/bin/zsh"
      }
    }
  }
}
